.PHONY: who
who:  ## 現在ログインしてる人一覧
	who

.PHONY: help
help:  ## コマンド一覧
	@echo "#######################################################################"
	@echo "# $$ make XXX で実行"
	@echo "# $$ make XXX --dry-run で打つコマンド確認可能"
	@echo "#######################################################################"
	@grep -E '^[a-zA-Z_-]+:.*?## .*$$' $(MAKEFILE_LIST) | awk 'BEGIN {FS = ":.*?## "}; {printf "\033[36m%-20s\033[0m %s\n", $$1, $$2}'
